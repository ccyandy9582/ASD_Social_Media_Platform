<%results.forEach((result, index)=>{%>
    user <%=index%>: <br>
    Name: <%=result.name%><br>
    Password: <%=result.pw%><br>
    email: <%=result.email%><br><br>
<%})%>

<form method="POST">
    <button type="submit">a</button>
</form>

<div class="card-deck">
    <% interest_list.forEach((interest)=> {%>
        <div class="card">
            <img class="card-img-top" src="<%=interest.img%>" alt="Card image cap">
            <div class="card-body">
                <h5 class="card-title"><%= interest.hobby %> </h5>
            </div>
        </div>
    <%})%>    
</div>
